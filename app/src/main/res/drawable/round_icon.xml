<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- rectangle表示为矩形 -->

    <!-- 填充的颜色 -->
    <solid android:color="#FFDAB9" />

    <!-- 边框的粗细和颜色 -->
    <stroke
        android:width="1sp"
        android:color="#FFDAB9"/>
    <!-- android:radius 圆角的半径 -->
    <corners
        android:radius="15sp"/>
</shape>